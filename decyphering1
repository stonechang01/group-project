b=["ABCDEFGHIJK","EFGHIJKLMNOP","BCDEFGHIJKL","OPQRSTUVWXYZ","OPQRSTUVWXYZ","KLMNOPQRSTUV"]

def findStart(i):
    start = ""
    for x in b:
        rtn = x[:i]
        for y in b:
            if (rtn in y[1:]):
                break
            elif (y == b[len(b)-1]):
                start = rtn
    return start


c = 8
start = findStart(c)

print(start)
